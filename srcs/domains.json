{
	"coupang": {
		"url": "https://www.coupang.com/np/search",
		"page_key": "page",
		"main_queries": [
			{
				"name": "q",
				"desc": "search keyword",
				"required": ["name", "value"]
			},
			{
				"name": "listSize",
				"desc": "item count per page",
				"values": [
					72,
					36,
					48,
					60
				],
				"required": ["name", "value"]
			},
			{
				"name": "sorter",
				"desc": "order by method",
				"values": [
					"scoreDesc",
					"latestAsc", 
					"salePriceAsc",
					"salePriceDesc",
					"saleCountDesc"
				],
				"required": ["name", "value"]
			},
			{
				"name": "page",
				"desc": "page number",
				"required": ["name", "value"]
			}
		],
		"selector": {
			"items": "li.search-product",
			"item" : {
				"image": "img.search-product-wrap-img",
				"name": "div.descriptions-inner > div.name",
				"price": "strong.price-value",
				"rating": "div.other-info > div.rating-star > span.star > em.rating",
				"reviews": "div.other-info > div.rating-star > span.rating-total-count"
			}
		}
	}
}

